{
    "name": "rafflesargentina/l5-automobile",
    "description": "Automobile Service Provider for Laravel 5",
    "keywords": [
        "pluma", "boostrap", "laravel", "city", "database", "eloquent"
    ],
    "homepage": "https://github.com/rafflesargentina/l5-automobile",
    "license": "MIT",
    "authors": [
        {
            "name": "Mario Patronelli",
            "email": "mario@raffles.com.ar",
            "homepage": "http://raffles.com.ar",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.6.4",
        "rafflesargentina/l5-resource-controller": "^0.1.6",
        "rafflesargentina/l5-filterable-sortable": "^0.1.3",
        "laracasts/presenter": "^0.2.1",
        "caffeinated/repository": "^3.1",
        "rafflesargentina/l5-action-based-form-request": "^0.1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "phpspec/phpspec": "^3.4",
        "squizlabs/php_codesniffer": "3.*",
        "laravel/laravel": "^5.4",
        "laravelcollective/html": "^5.4",
        "orchestra/testbench": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "RafflesArgentina\\ArgentineCity\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "RafflesArgentina\\ArgentineCity\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
